{
  "name": "@dodona/dolos",
  "version": "2.1.0",
  "description": "Code similarity detection based on the Winnowing algorithm",
  "main": "dist/index.js",
  "engines": {
    "node": ">12"
  },
  "bin": {
    "dolos": "dist/cli.js"
  },
  "scripts": {
    "dolos": "yarn build && node dist/cli.js",
    "debug": "yarn build && node inspect dist/cli.js",
    "build": "tsc --build --verbose",
    "force-build": "tsc --build --verbose --force",
    "lint": "eslint --ext .ts src/"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/dodona-edu/dolos.git"
  },
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "devDependencies": {
    "@types/express": "4.17.17",
    "@types/node": "18.14.6",
    "@typescript-eslint/eslint-plugin": "5.52.0",
    "@typescript-eslint/parser": "5.52.0",
    "eslint": "8.35.0",
    "np": "7.6.3",
    "ts-node": "10.9.1",
    "typescript": "4.9.5"
  },
  "dependencies": {
    "@dodona/dolos-lib": "2.1.0",
    "@dodona/dolos-web": "2.1.0",
    "@elm-tooling/tree-sitter-elm": "^5.6.0",
    "chalk": "^4.1.1",
    "cliui": "^8.0.0",
    "commander": "^10.0.0",
    "csv-stringify": "^6.2.0",
    "express": "^4.17.1",
    "open": "^8.2.1",
    "tree-sitter": "^0.20.0",
    "tree-sitter-bash": "^0.19.0",
    "tree-sitter-c": "^0.20.1",
    "tree-sitter-c-sharp": "^0.20.0",
    "tree-sitter-cpp": "^0.20.0",
    "tree-sitter-java": "^0.19.1",
    "tree-sitter-javascript": "^0.19.0",
    "tree-sitter-php": "^0.19.0",
    "tree-sitter-python": "^0.20.1",
    "tree-sitter-typescript": "^0.20.0"
  },
  "bugs": {
    "url": "https://github.com/dodona-edu/dolos/issues"
  },
  "homepage": "https://dolos.ugent.be",
  "keywords": [
    "plagiarism",
    "plagiarism-checker",
    "plagiarism detection",
    "similarity",
    "code similarity"
  ]
}
